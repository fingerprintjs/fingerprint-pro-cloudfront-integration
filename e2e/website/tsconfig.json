{
  "files": [
    "index.ts"
  ],
  "compilerOptions": {
    "target": "es2015",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": false,
    "resolveJsonModule": true,
    "lib": [
      "dom"
    ]
  },
  "exclude": [
    "website-dist"
  ]
}
