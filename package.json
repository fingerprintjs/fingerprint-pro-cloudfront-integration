{
  "name": "@fingerprintjs/fingerprint-pro-cloudfront-lambda-function",
  "version": "1.4.1-rc.1",
  "description": "Fingerprint Pro CloudFront lambda@edge function",
  "author": "FingerprintJS, Inc (https://fingerprint.com)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf dist && NODE_OPTIONS=\"--max_old_space_size=4098\" rollup -c rollup.config.js --bundleConfigAsCjs",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --max-warnings 0 .",
    "lint:fix": "pnpm lint --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:dts": "tsc --noEmit --isolatedModules dist/fingerprintjs-pro-cloudfront-lambda-function.d.ts"
  },
  "main": "dist/fingerprintjs-pro-cloudflare-lambda-function.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.224.0",
    "@aws-sdk/client-lambda": "^3.224.0",
    "@aws-sdk/client-secrets-manager": "^3.468.0",
    "punycode": "^2.1.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.515.0",
    "@commitlint/config-conventional": "^17.0.3",
    "@fingerprintjs/commit-lint-dx-team": "^0.0.2",
    "@fingerprintjs/conventional-changelog-dx-team": "^0.1.0",
    "@fingerprintjs/eslint-config-dx-team": "^0.1.0",
    "@fingerprintjs/prettier-config-dx-team": "^0.1.0",
    "@fingerprintjs/tsconfig-dx-team": "^0.0.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/aws-lambda": "^8.10.107",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.9",
    "@types/punycode": "^2.1.0",
    "adm-zip": "^0.5.10",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": "^13.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-license": "^2.8.1",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "whatwg-fetch": "^3.6.2"
  },
  "lint-staged": {
    "*.ts": "pnpm run lint:fix",
    "*.tsx": "pnpm run lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
